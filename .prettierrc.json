{
  "endOfLine": "lf",
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 100,
  "trailingComma": "none",
  "tailwindStylesheet": "./src/styles/global.css",
  "tailwindFunctions": ["cn", "cva"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "^(solid-js/(.*)$)|^(solid-js$)",
    "^(@solidjs/(.*)$)",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^~/(.*)$",
    "",
    "^[.]"
  ],
  "importOrderCaseSensitive": false,
  "plugins": ["prettier-plugin-tailwindcss", "@ianvs/prettier-plugin-sort-imports"]
}
